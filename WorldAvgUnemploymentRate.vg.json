{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 500,
    "title": "World Average Unemployment Rate Across the year (2011-2021)",
    "data": {
        "url": "https://raw.githubusercontent.com/kokbanyap/3179_Assignment2/main/unemployment_dataset.csv"
    },
    "layer": [{
        "mark":"line",
        "encoding": {
            "x": {"field": "Year", "type":"temporal","title":"Year"},
            "y": {"field": "YearAvgUnemployment", "type": "quantitative","title":"World Average Unemployment Rate (%)"}
        }},{
        "encoding":{
            "x": {"aggregate":"max", "field":"Year", "type":"temporal"},
            "y":{"aggregate":{"argmax":"Year"}, "field":"YearAvgUnemployment", "type":"quantitative"}
        },
        "layer":[{
            "mark":{"type": "circle"}
        }, {
            "mark": {
                "type": "text", "align":"right", "dx":4, "radius": 8
            },
            "encoding": {
                "text":{"field": "YearAvgUnemployment", "type": "quantitative"}
            }
        }]
    }],
    "config": {"view": {"stroke":null}}
}    

